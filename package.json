{
  "name": "platformatic-starter",
  "version": "0.1.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "npx platformatic db compile",
    "dev": "concurrently 'npm:migrate' 'npm:start'",
    "format": "prettier --write \"**/*.ts\" \"*.ts\"",
    "generate": "npx platformatic db migrations generate",
    "postinstall": "npx husky install",
    "lint": "eslint \"**/*.ts\" \"*.ts\" --fix",
    "migrate": "npx platformatic db migrations apply",
    "service": "npx platformatic service start",
    "start": "npx platformatic db start",
    "typecheck": "tsc --noEmit"
  },
  "nano-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@fastify/helmet": "11.1.1",
    "@fastify/rate-limit": "8.0.3",
    "@platformatic/sql-graphql": "1.2.0",
    "@platformatic/sql-mapper": "1.2.0",
    "ajv": "8.12.0",
    "fastify": "4.23.2",
    "fluent-json-schema": "4.1.2",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/node": "20.8.3",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "concurrently": "8.2.1",
    "eslint": "8.51.0",
    "eslint-config-productsway": "^1.3.0",
    "husky": "8.0.3",
    "nano-staged": "0.8.0",
    "platformatic": "1.2.0",
    "prettier": "3.0.3",
    "sort-package-json": "2.6.0"
  },
  "engines": {
    "node": "^16.17.0 || ^18.8.0 || >=20"
  }
}
