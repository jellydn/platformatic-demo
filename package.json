{
  "name": "platformatic-starter",
  "version": "0.1.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "npx platformatic db compile",
    "dev": "concurrently 'npm:migrate' 'npm:start'",
    "format": "prettier --write \"**/*.ts\" \"*.ts\"",
    "generate": "npx platformatic db migrations generate",
    "postinstall": "npx husky install",
    "lint": "eslint \"**/*.ts\" \"*.ts\" --fix",
    "migrate": "npx platformatic db migrations apply",
    "service": "npx platformatic service start",
    "start": "npx platformatic db start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/helmet": "11.0.0",
    "@fastify/rate-limit": "8.0.3",
    "@platformatic/sql-graphql": "0.35.4",
    "@platformatic/sql-mapper": "0.35.4",
    "ajv": "8.12.0",
    "fastify": "4.21.0",
    "fluent-json-schema": "4.1.1",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/node": "20.5.0",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "concurrently": "8.2.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-productsway": "^1.2.0",
    "eslint-config-xo": "0.43.1",
    "eslint-config-xo-typescript": "0.57.0",
    "husky": "8.0.3",
    "nano-staged": "0.8.0",
    "platformatic": "0.35.4",
    "prettier": "3.0.2",
    "sort-package-json": "2.5.1"
  },
  "engines": {
    "node": "^16.17.0 || ^18.8.0 || >=19"
  },
  "nano-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write",
    "package.json": "sort-package-json"
  }
}
